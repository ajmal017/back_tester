<table>
  <tr>
    <th>Rank</th>
    <th>Ticker</th>
    <th>Description</th>
    <th>Ranked Return</th>
    <th>Last Price</th>
    <th>PC21</th>
  </tr>
  <% i = 0 %>
  <% @returns.each do |k,v| %>
    <% i += 1 %>
    <tr>
      <td><%= i %></td>
      <td><%= k %></td>
      <td><%= @securities[k].description %></td>
      <td><%= number_to_percentage(v * 100.0) %></td>
      <td><%= number_to_currency(@securities[k].last_price) %></td>
      <td><%= number_to_currency(@securities[k].price_channel(:length => 21)[:upper]) %></td>
    </tr>  
  <% end # each %>
</table>